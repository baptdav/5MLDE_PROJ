{
    "meta": {
        "title": "Direct Marketing Campaigns Dataset Expectations",
        "description": "Expectations for a dataset related to direct marketing campaigns of a Portuguese banking institution.",
        "author": "BBL",
        "created_at": "2024-01-19"
    },
    "expectations": [
        {
            "expectation_type": "expect_column_values_to_be_in_set",
            "kwargs": {
                "column": "job",
                "value_set": [
                    "admin.",
                    "blue-collar",
                    "technician",
                    "services",
                    "management",
                    "retired",
                    "entrepreneur",
                    "self-employed",
                    "housemaid",
                    "unemployed",
                    "student",
                    "unknown"
                ]
            },
            "meta": {
                "description": "Job types of clients",
                "notes": "Valid job types include admin., blue-collar, technician, services, management, retired, entrepreneur, self-employed, housemaid, unemployed, student, unknown."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_in_set",
            "kwargs": {
                "column": "housing",
                "value_set": [
                    "yes",
                    "no",
                    "unknown"
                ]
            },
            "meta": {
                "description": "Housing loan status",
                "notes": "Valid values include yes, no, unknown."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_in_set",
            "kwargs": {
                "column": "loan",
                "value_set": [
                    "yes",
                    "no",
                    "unknown"
                ]
            },
            "meta": {
                "description": "Personal loan status",
                "notes": "Valid values include yes, no, unknown."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_in_set",
            "kwargs": {
                "column": "default",
                "value_set": [
                    "yes",
                    "no"
                ]
            },
            "meta": {
                "description": "Credit default status",
                "notes": "Valid values include yes, no."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_in_set",
            "kwargs": {
                "column": "y",
                "value_set": [
                    "yes",
                    "no"
                ]
            },
            "meta": {
                "description": "Subscription to term deposit",
                "notes": "Valid values include yes, no."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_of_type",
            "kwargs": {
                "column": "age",
                "type_": "int"
            },
            "meta": {
                "description": "Age of clients",
                "notes": "Age should be an integer."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_between",
            "kwargs": {
                "column": "age",
                "min_value": 18,
                "max_value": 125
            },
            "meta": {
                "description": "Valid age range",
                "notes": "Age should be between 18 (not minor) and 125 (inclusive)."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_of_type",
            "kwargs": {
                "column": "campaign",
                "type_": "int"
            },
            "meta": {
                "description": "Number of contacts performed during this campaign",
                "notes": "Campaign should be an integer."
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_non_negative",
            "kwargs": {
                "column": "campaign"
            },
            "meta": {
                "description": "Non-negative campaign values",
                "notes": "Campaign values should be non-negative."
            }
        },
        {
            "expectation_type": "expect_column_values_to_not_be_null",
            "kwargs": {
                "column": "education"
            },
            "meta": {
                "description": "Education level of clients",
                "notes": "Education should not be null."
            }
        },
        {
            "expectation_type": "expect_table_row_count_to_be_between",
            "kwargs": {
                "min_value": 1
            },
            "meta": {
                "description": "Minimum number of rows",
                "notes": "The dataset should have at least one row."
            }
        }
    ]
}